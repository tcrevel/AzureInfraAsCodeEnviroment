		"ghcr.io/devcontainers/features/github-cli:1": {},
		"ghcr.io/devcontainers/features/terraform:1": {},
		"ghcr.io/natescherer/devcontainers-custom-features/powershell-resources:1": {
			"resources": "Az"
		},
		"ghcr.io/natescherer/devcontainers-custom-features/k9s:1": {
			"version": "latest"
		}
	},
	"extensions": [
		"ms-azuretools.vscode-bicep",
		"ms-azuretools.vscode-azurevirtualmachines",
		"ms-dotnettools.vscode-dotnet-runtime",
		"bencoleman.armview",
		"ms-vscode.azure-account",
		"ms-azuretools.vscode-azureappservice",
		"ms-vscode.azurecli",
		"ms-azuretools.vscode-cosmosdb",
		"ms-azuretools.azure-dev",
		"ms-azuretools.vscode-azurefunctions",
		"ms-kubernetes-tools.vscode-aks-tools",
		"ms-azuretools.vscode-logicapps",
		"ms-azuretools.vscode-azurelogicapps",
		"ms-azure-devops.azure-pipelines",
		"AzurePolicy.azurepolicyextension",
		"msazurermtools.azurerm-vscode-tools",
		"ms-azuretools.vscode-azureresourcegroups",
		"ms-azuretools.vscode-azurestorage",
		"ms-vscode.vscode-node-azure-pack",
		"Azurite.azurite",
		"ms-azuretools.vscode-docker",
		"mhutchie.git-graph",
		"donjayamanne.githistory",
		"eamodio.gitlens",
		"hashicorp.terraform",
		"J-Cat.vscode-logicapp-runner",
		"yzhang.markdown-all-in-one",
		"DavidAnson.vscode-markdownlint",
		"ms-vscode.powershell",
