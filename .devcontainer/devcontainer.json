{
	"name": "Azure Development environment",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"forwardPorts": [ 7071 ],
	"remoteUser": "vscode",
	"features": {
		"ghcr.io/devcontainers/features/dotnet:1": {
			"version": "3.1",
			"runtimeOnly": true
		},
		"ghcr.io/devcontainers/features/dotnet": {
			"version": "2.1",
			"runtimeOnly": true
		},
		"ghcr.io/devcontainers/features/powershell": "7.3.1",
		"ghcr.io/devcontainers/features/azure-cli": {
			"installBicep": true,
			"version": "2.43.0"
		},
		"ghcr.io/jlaundry/devcontainer-features/azure-functions-core-tools": {
			"version": "4.0.4915"
		},
		"ghcr.io/rchaganti/vsc-devcontainer-features/azurebicep:1": {
			"version": "0.13.1"
		},
		"ghcr.io/devcontainers/features/github-cli:1": {
			"version": "2.21.1"
		},
		"ghcr.io/devcontainers/features/terraform:1": {
			"version": "1.3.6",
			"tflint": "0.44.0",
			"terragrunt": "0.42.5",
			"installTFsec": true,
			"installTerraformDocs": true
		},
		"ghcr.io/natescherer/devcontainers-custom-features/powershell-resources:1": {
			"resources": "Az"
		},
		"ghcr.io/natescherer/devcontainers-custom-features/k9s:1": {
			"version": "latest"
		},
		"ghcr.io/devcontainers/features/java": "17",
		"ghcr.io/devcontainers/features/node": "18",
		"ghcr.io/devcontainers/features/python": "3.11",
		"ghcr.io/rio/features/k3d": {
			"version": "5.4.6"
		},
		"ghcr.io/devcontainers/features/docker-from-docker": {
			"version": "20.10",
			"dockerDashComposeVersion": "v1"
		},
		"ghcr.io/devcontainers/features/aws-cli": {
			"version": "2.9.11"
		}
	},
	"extensions": [
		"ms-azuretools.vscode-bicep",
		"ms-azuretools.vscode-azurevirtualmachines",
		"ms-dotnettools.vscode-dotnet-runtime",
		"bencoleman.armview",
		"ms-vscode.azure-account",
		"ms-azuretools.vscode-azureappservice",
		"ms-vscode.azurecli",
		"ms-azuretools.vscode-cosmosdb",
		"ms-azuretools.azure-dev",
		"ms-azuretools.vscode-azurefunctions",
		"ms-kubernetes-tools.vscode-aks-tools",
		"ms-azuretools.vscode-logicapps",
		"ms-azuretools.vscode-azurelogicapps",
		"ms-azure-devops.azure-pipelines",
		"AzurePolicy.azurepolicyextension",
		"msazurermtools.azurerm-vscode-tools",
		"ms-azuretools.vscode-azureresourcegroups",
		"ms-azuretools.vscode-azurestorage",
		"ms-vscode.vscode-node-azure-pack",
		"Azurite.azurite",
		"ms-azuretools.vscode-docker",
		"mhutchie.git-graph",
		"donjayamanne.githistory",
		"eamodio.gitlens",
		"hashicorp.terraform",
		"J-Cat.vscode-logicapp-runner",
		"yzhang.markdown-all-in-one",
		"DavidAnson.vscode-markdownlint",
		"ms-vscode.powershell",
		"ms-python.python",
		"ms-dotnettools.csharp",
		"ms-vsliveshare.vsliveshare",
		"quicktype.quicktype",
		"humao.rest-client",
		"ms-dotnettools.csharp",
		"vscjava.vscode-java-debug"
	],
	"customizations": {
		"codespaces": {
			"openFiles": [
				"README.md"
			]
		}
	}
}